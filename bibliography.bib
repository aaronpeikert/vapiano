  file = {Full Text PDF:C\:\\Users\\Aaron\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\36rj7y3p.default\\zotero\\storage\\73ANBEJH\\Edwards and Templeton - 2005 - The structure of perceived qualities of situations.pdf:application/pdf;Snapshot:C\:\\Users\\Aaron\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\36rj7y3p.default\\zotero\\storage\\DV8I6KUC\\abstract.html:text/html}
}

@article{sherman_properties_2012,
  title = {Properties of Persons and Situations Related to Overall and Distinctive Personality-Behavior Congruence},
  volume = {46},
  issn = {0092-6566},
  url = {http://www.sciencedirect.com/science/article/pii/S009265661100167X},
  doi = {10.1016/j.jrp.2011.12.006},
  abstract = {Congruence is the degree to which one’s personality matches one’s behavior in a particular situation. On four separate occasions over several weeks, 202 undergraduate participants described a situation they encountered the previous day and their behavior. Analyses considered overall congruence as well as distinctive congruence, adjusted for the match of personality and behavior to the normative personality and behavior profile. Overall congruence was strongly associated with better psychological adjustment; distinctive congruence was not. Similarly, situation strength and affordances for autonomy, relatedness to others, and competence were strongly linked to overall congruence, but only weakly associated with distinctive congruence. Behaving in accordance with one’s true self is only related to positive psychological outcomes when it is accordance with normative standards.},
  timestamp = {2017-07-12T12:03:38Z},
  number = {1},
  journaltitle = {Journal of Research in Personality},
  shortjournal = {Journal of Research in Personality},
  author = {Sherman, Ryne A. and Nave, Christopher S. and Funder, David C.},
  date = {2012-02-01},
  pages = {87--101},
  keywords = {Adjustment,Congruence,Distinctiveness,Normativeness,Self-Determination Theory,Situation strength},
  file = {ScienceDirect Snapshot:C\:\\Users\\Aaron\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\36rj7y3p.default\\zotero\\storage\\QKH93VNK\\S009265661100167X.html:text/html}
}

@article{sherman_situational_2010-1,
  title = {Situational Similarity and Personality Predict Behavioral Consistency},
  volume = {99},
  rights = {(c) 2016 APA, all rights reserved},
  issn = {1939-1315 0022-3514},
  doi = {10.1037/a0019796},
  abstract = {A new method for assessing situations is employed to examine the association between situational similarity, personality, and behavioral consistency across ecologically representative contexts. On 4 occasions across 4 weeks, 202 undergraduate participants (105 women, 97 men) wrote descriptions of a situation they had experienced the previous day. In addition, they rated its psychological features using the recently developed Riverside Situational Q-Sort (RSQ) Version 2.0 (Wagerman \& Funder, 2009) and their behavior using the Riverside Behavioral Q-Sort (RBQ) Version 3.0 (Funder, Furr, \& Colvin, 2000; Furr, Wagerman, \& Funder, 2010). Independent judges also rated the situations using the RSQ, on the basis of the participants' written descriptions. Results indicated (a) participants' ratings of their behavior were impressively consistent across the 4 situations; (b) the 4 situations experienced by a single participant tended to be described more similarly to each other than to situations experienced by different participants; (c) situational similarity, especially from the individual's own point of view, strongly predicted behavioral consistency; and (d) personality characteristics predicted behavioral consistency even after controlling for situational similarity. Relatively consistent persons described themselves as ethically consistent, conservative, calm and relaxed, and low on neuroticism. These results imply that behavioral consistency in daily life stems from multiple sources, including situation selection and the distinctive influence of personality, and further suggest that tools for situational assessment such as the RSQ can have wide utility.},
  timestamp = {2017-07-12T12:04:21Z},
  langid = {english},
  number = {2},
  journaltitle = {Journal of Personality and Social Psychology},
  author = {Sherman, Ryne A. and Nave, Christopher S. and Funder, David C.},
  date = {2010},
  pages = {330--343},
  keywords = {*Behavior,*Personality,Life Experiences,Psychological Assessment}
}

