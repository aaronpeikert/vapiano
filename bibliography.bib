@article{tyler_time-sampling:_1979,
  title = {Time-Sampling: {{A}} Matter of Convention},
  volume = {27},
  issn = {0003-3472},
  url = {http://www.sciencedirect.com/science/article/pii/0003347279900162},
  doi = {10.1016/0003-3472(79)90016-2},
  shorttitle = {Time-Sampling},
  abstract = {The methodological problems associated with the time-sampling of behaviour are reviewed. Time-sampling conventions are investigated by means of (i) an analysis of event recorded observations and (ii) a computer simulation. The abilities of each convention to approximate to the actual value on measures of frequency, duration and bout-length are compared. For each comparison the distributions of the bouts of behaviour and the inter-bout intervals are known. Of the conventions compared, ‘one-zero’ sampling appears to be the least satisfactory. ‘Instantaneous’ sampling and ‘predominant activity’ sampling give better approximations under most circumstances. However, depending upon the parameters of the behaviour studied, all three conventions can give accurate or inaccurate estimates on particular measures. Recommendations concerning the use of the conventions are made.},
  timestamp = {2017-07-12T11:37:33Z},
  journaltitle = {Animal Behaviour},
  shortjournal = {Animal Behaviour},
  author = {Tyler, Stephen},
  date = {1979-08-01},
  pages = {801--810},
  file = {ScienceDirect Full Text PDF:C\:\\Users\\Aaron\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\36rj7y3p.default\\zotero\\storage\\PII7QIAE\\Tyler - 1979 - Time-sampling A matter of convention.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Aaron\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\36rj7y3p.default\\zotero\\storage\\D7BDE8FW\\0003347279900162.html:text/html}
}

@article{edwards_structure_2005,
  title = {The Structure of Perceived Qualities of Situations},
  volume = {35},
  issn = {1099-0992},
  url = {http://onlinelibrary.wiley.com/doi/10.1002/ejsp.271/abstract},
  doi = {10.1002/ejsp.271},
  abstract = {Situations can be seen as having attributes or qualities in much the same way as people have traits. The structure of people's perceptions of these situation qualities was explored. A comprehensive list of adjectives that might describe situations was generated, and people rated situations using samples of the words. Across several samples of words and participants and several analytic methods, four factors show up regularly (positivity, negativity, productivity, and ease of negotiation). In a second study, it was shown that these factors predict the way in which people freely sort situations. The conceptual nature of these factors and of situation qualities is discussed, with particular emphasis on how people's goals and perceived outcomes influence their perceptions of situations. Copyright © 2005 John Wiley \& Sons, Ltd.},
  timestamp = {2017-07-12T11:50:15Z},
  langid = {english},
  number = {6},
  journaltitle = {European Journal of Social Psychology},
  shortjournal = {Eur. J. Soc. Psychol.},
  author = {Edwards, John A. and Templeton, Angela},
  date = {2005-11-01},
